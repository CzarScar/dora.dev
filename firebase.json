{
  "hosting": {
    "public": "hugo/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
        {
            "source": "/quickcheck.html",
            "destination": "/quickcheck/",
            "type": 302
        },
        {
            "source": "/performance.html:querystring*",
            "destination": "/quickcheck/2019/results/:querystring",
            "type": 302
        }
    ]
  }
}
